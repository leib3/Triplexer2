#include <EEPROM.h>

void setup() {
  // put your setup code here, to run once:
   Serial.begin(9600);
}

struct settingsStruct
{
   uint8_t xCCNum;
   uint8_t xChannelNum;
   uint8_t yCCNum;
   uint8_t yChannelNum;
   uint8_t totCCNum;
   uint8_t totChannelNum;
   bool xInv;
   bool yInv;
   bool totInv;
};

   
void loop() {
  // put your main code here, to run repeatedly:
 
}



void Store(struct settingsStruct Data, int Addr){
  Addr=(Addr-1)*9;
  EEPROM.put(Addr, Data);
}

struct settingsStruct load(int Addr){
  struct settingsStruct Data;
  Addr=(Addr-1)*9;
  EEPROM.get(Addr, Data);
  return Data;
}
